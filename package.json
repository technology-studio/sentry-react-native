{
  "name": "@txo/sentry-react-native",
  "version": "2.0.4",
  "description": "Technology Studio - Sentry react native",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/sentry-react-native.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --max-warnings 0 .",
    "fixcode": "eslint . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky install && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint && yarn build && tsc --noEmit && yarn test --coverage && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@txo/log": "^2.0.14",
    "@txo/types": "^1.7.0"
  },
  "peerDependencies": {
    "@sentry/react-native": "^5.13.0",
    "@txo-peer-dep/log": "^4.0.3",
    "@txo-peer-dep/observable-redux": "^2.0.1",
    "@txo-peer-dep/redux": "^1.0.1",
    "react-native": "*"
  },
  "devDependencies": {
    "@apollo/client": "^3.8.7",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@react-navigation/native": "^6.1.9",
    "@sentry/react-native": "^5.13.0",
    "@txo-peer-dep/log": "^4.0.3",
    "@txo-peer-dep/observable-redux": "^2.0.1",
    "@txo-peer-dep/redux": "^1.0.1",
    "@txo/commitlint": "^1.0.7",
    "@txo/log-console": "^3.0.0",
    "@txo/semantic-release": "^1.0.2",
    "@types/jest": "^29.5.8",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-native": "^0.72.6",
    "concurrently": "^8.2.2",
    "eslint-config-txo-typescript-react": "^2.0.5",
    "graphql": "^16.8.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.1.0",
    "npm-check-updates": "^16.14.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.7",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
