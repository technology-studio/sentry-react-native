{
  "name": "@txo/sentry-react-native",
  "version": "1.2.13",
  "description": "Technology Studio - Sentry react native",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/technology-studio/sentry-react-native.git"
  },
  "author": {
    "name": "Technology Studio",
    "email": "npm@technologystudio.sk",
    "url": "https://github.com/technology-studio/"
  },
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "build": "yarn build:clean && yarn build:lib",
    "build:clean": "yarn rimraf lib",
    "build:lib": "yarn tsc",
    "build:watch": "yarn tsc --watch",
    "test": "jest",
    "test:watch": "concurrently \"yarn build:watch\" \"jest --watch\"",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "compare-boilerplate-version": "./scripts/compare-boilerplate-version.sh",
    "lint": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx .",
    "fixcode": "eslint --format=node_modules/eslint-formatter-pretty --ext .jsx,.js,.ts,.tsx . --fix",
    "git-hook": "yarn -s lint-staged",
    "prepare": "husky install && yarn build",
    "print-txo-packages": "yarn list 2> /dev/null|grep @txo|sed 's/.*\\(@txo[^@]*\\)@^*\\([^ ]*\\).*/\\1@\\2/g'|sort|uniq",
    "sanity": "yarn lint && yarn build && tsc --noEmit && yarn test && yarn compare-boilerplate-version && echo 'success'",
    "semantic-release": "semantic-release",
    "update-boilerplate-version": "./scripts/update-boilerplate-version.sh"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@txo/log": "^2.0.1",
    "@txo/types": "^1.3.0"
  },
  "peerDependencies": {
    "@sentry/react-native": "^5.7.1",
    "@txo-peer-dep/log": "^4.0.0",
    "@txo-peer-dep/observable-redux": "^2.0.1",
    "@txo-peer-dep/redux": "^1.0.1",
    "react-native": "*",
    "react-redux": "*",
    "redux": "*"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.17",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@sentry/react-native": "^5.7.1",
    "@txo-peer-dep/log": "^4.0.0",
    "@txo-peer-dep/observable-redux": "^2.0.1",
    "@txo-peer-dep/redux": "^1.0.1",
    "@txo/log-console": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-native": "^0.72.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-txo-typescript-react": "^1.2.1",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-redux-saga": "^1.3.2",
    "graphql": "^16.7.1",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "npm-check-updates": "^16.10.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-navigation": "^4.4.4",
    "react-redux": "^8.1.1",
    "redux": "^4.2.1",
    "redux-saga": "^1.2.3",
    "rimraf": "^5.0.1",
    "semantic-release": "^21.0.7",
    "semantic-release-slack-bot": "^4.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
